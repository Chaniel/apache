#! /bin/sh
#
# Created by configure

"./configure" \
"--prefix=/data/apache" \
"--with-pcre=/data/libs/pcre" \
"--with-included-apr" \
"--enable-so" \
"--enable-ssl" \
"--enable-cgi" \
"--enable-rewrite" \
"--with-zlib" \
"--with-pcre=/data/libs/pcre/" \
"--enable-proxy" \
"--enable-proxy-http" \
"--enable-proxy-ajp" \
"$@"
